    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.2.0",
    "nock": "^10.0.6",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.4",
